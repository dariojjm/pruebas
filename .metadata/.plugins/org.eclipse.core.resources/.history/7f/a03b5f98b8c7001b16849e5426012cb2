package modelo;

public class Cuadrado  {
	private int lado;
	public Cuadrado(int lado) {
		this.lado=lado;
	}
	public void setLado(int lado) {
		this.lado = lado;
	}
	public int calcularArea() {
		// TODO Auto-generated method stub
		return lado-lado;
	}

	public int calcularPerimetro() {
		// TODO Auto-generated method stub
		return 0;
	}

}
